{
    "name": "sgt-watch",
    "displayName": "SGT-WATCH",
    "description": "Monitors main.dart with military-themed sergeant chatter and Believing Soldier encouragement",
    "version": "0.0.1",
    "publisher": "BiqqMax",
    "engines": {
        "vscode": "^1.97.0"
    },
    "categories": [
        "Programming Languages",
        "Other"
    ],
    "activationEvents": [
        "onLanguage:dart"
    ],
    "main": "./dist/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run esbuild",
        "esbuild": "esbuild ./src/extension.ts --bundle --outfile=dist/extension.js --external:vscode --format=cjs --platform=node",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^20.12.12",
        "@types/vscode": "^1.97.0",
        "esbuild": "^0.23.0",
        "typescript": "^5.4.5"
    },
    "dependencies": {}
}